#!/bin/sh

case "$root" in
	ZFS|ZFS:|ZFS:AUTO|ZFS:auto)
		root="ZFS:auto"
		ZFSROOT="true"
		rootok=1
		SEEK_BOOTFS=1
		;;

	ZFS:*|ZFS=*)
		root="${root:4}"
		ZFSROOT="true"
		rootok=1
		SEEK_BOOTFS=0
		;;
esac

if [ "$ZFSROOT" = "true" ] ; then
	info "ZFS: root = $root"
	info "ZFS: SEEK_BOOTFS = $SEEK_BOOTFS"
fi
