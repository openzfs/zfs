[global]
filename_format=file$jobnum
nrfiles=16
group_reporting=1
fallocate=0
overwrite=0
thread=1
rw=randrw
rwmixread=80
time_based=1
directory=${DIRECTORY}
runtime=${RUNTIME}
bssplit=4k/50:8k/30:128k/10:1m/10
ioengine=psync
sync=${SYNC_TYPE}
direct=${DIRECT}
numjobs=${NUMJOBS}
randseed=${RANDSEED}
buffer_compress_percentage=${COMPPERCENT}
buffer_pattern=0xdeadbeef
buffer_compress_chunk=${COMPCHUNK}

[job]
