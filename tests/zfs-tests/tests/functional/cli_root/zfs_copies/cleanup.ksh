. $STF_SUITE/tests/functional/cli_root/zfs_copies/zfs_copies.cfg

#
# umount the ufs|ext fs if there is timedout in the ufs|ext test
#

if ismounted $FS_MNTPOINT $NEWFS_DEFAULT_FS ; then
	log_must umount -f $FS_MNTPOINT
	rm -fr $FS_MNTPOINT
fi

default_cleanup
