# Makefile.in for spl kernel module

MODULE := spl
EXTRA_CFLAGS = $(ZFS_MODULE_CFLAGS) @KERNELCPPFLAGS@

# Solaris porting layer module
obj-$(CONFIG_ZFS) := $(MODULE).o

$(MODULE)-objs += @top_srcdir@/module/spl/spl-debug.o
